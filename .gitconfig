[user]
	name = Yushi Cui
	email = realYushi@gmail.com
[init]
    defaultBranch = main	
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore_global

[push]
    default = current

[help]
    autocorrect = 1

[credential]
    helper = osxkeychain
[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true  


[merge]
    conflictstyle = zdiff3